{
  "name": "vue-movie",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "vue-tsc --noEmit",
    "build:prod": "npm run type-check && npm run lint:check && vite build",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "docker:build": "docker build -t vue-movie .",
    "docker:run": "docker run -p 3000:80 vue-movie",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@iconify/vue": "^5.0.0",
    "@vueuse/core": "^13.6.0",
    "axios": "^1.11.0",
    "element-plus": "^2.10.6",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "pinyin-pro": "^3.27.0",
    "swiper": "^12.0.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue3-lazy": "1.0.0-alpha.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "globals": "^16.3.0",
    "husky": "^8.0.0",
    "less": "^4.4.0",
    "lint-staged": "^16.1.5",
    "prettier": "3.6.2",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
